generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model GuildConfig {
  id                String   @id @default(cuid())
  guildId          String   @unique
  representorsRoleId String
  logChannelId     String?
  createdAt        DateTime @default(now())
  updatedAt        DateTime @updatedAt
} 